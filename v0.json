{
  "version": 1,
  "project": "salud-language-learning",
  "framework": "python",
  "buildCommand": "pip install -r requirements.txt",
  "devCommand": "python app.py",
  "routes": [
    { "src": "/(.*)", "dest": "/app.py" }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_APP": "app.py"
  }
}
